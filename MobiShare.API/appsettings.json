{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=mobishare.db"
  },
  "Jwt": {
    "Key": "your-secret-key-here-should-be-at-least-32-characters-long-for-security",
    "Issuer": "MobiShare.API",
    "Audience": "MobiShare.Users"
  },
  "IoTGateway": {
    "BaseUrl": "http://localhost:5001",
    "ApiKey": "your-gateway-api-key",
    "TimeoutSeconds": 30
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "MobiShare.IoT": "Debug",
      "MobiShare.Infrastructure.Services.HttpMqttService": "Debug"
    }
  },
  "AllowedHosts": "*"
}